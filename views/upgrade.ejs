<html lang="en">

<head>
    <meta charset="UTF-8">
    <title>GIS</title>
    <link rel="stylesheet" type="text/css" href="../css/bootstrap.css">
    <!-- Font Icon -->
    <link rel="stylesheet" href="fonts/material-icon/css/material-design-iconic-font.min.css">

    <!-- Main css -->
    <link rel="stylesheet" href="css/style.css">
    <link rel="stylesheet" href="css/main.css">

</head>

<body>
    <div class="main">

        <section class="pricing py-5">
            <div class="container">
                <h5 style="text-align: center; color: red;">Xin lỗi! Chúng tôi đang cải thiện tính năng này...</h5>
                <div  style="text-align: center;"><a href="/home/dashboard">Về trang chủ</a></div>
                <div class="row">
                    <!-- Free Tier -->
                    <div class="col-lg-6">
                        <div class="card mb-5 mb-lg-0">
                            <div class="card-body">
                                <h5 class="card-title text-muted text-uppercase text-center">Miễn phí</h5>
                                <h6 class="card-price text-center">$0<span class="period">/tháng</span></h6>
                                <hr>
                                <ul class="fa-ul">
                                    <li><span class="fa-li"><i class="fas fa-check"></i></span>Xem dữ liệu tổng quát
                                    </li>
                                    <li class="text-muted"><span class="fa-li"><i class="fas fa-times"></i></span>Xem dữ
                                        liệu chi tiết</li>
                                    <li class="text-muted"><span class="fa-li"><i class="fas fa-times"></i></span>Cho
                                        phép tải dữ liệu</li>
                                    <li class="text-muted"><span class="fa-li"><i class="fas fa-times"></i></span>Hỗ trợ
                                        24/7</li>
                                </ul>
                                <a href="/home/dashboard" class="btn btn-block btn-primary text-uppercase">Chọn</a>
                            </div>
                        </div>
                    </div>

                    <!-- Pro Tier -->
                    <div class="col-lg-6">
                        <div class="card">
                            <div class="card-body">
                                <h5 class="card-title text-muted text-uppercase text-center">VIP</h5>
                                <h6 class="card-price text-center">$69<span class="period">/tháng</span></h6>
                                <hr>
                                <ul class="fa-ul">
                                    <li><span class="fa-li"><i class="fas fa-check"></i></span>Xem dữ liệu tổng quát
                                    </li>
                                    <li><span class="fa-li"><i class="fas fa-check"></i></span>Xem dữ liệu chi tiết</li>
                                    <li><span class="fa-li"><i class="fas fa-check"></i></span>Cho phép tải dữ liệu</li>
                                    <li><span class="fa-li"><i class="fas fa-check"></i></span>Hỗ trợ 24/7</li>
                                </ul>
                                <a href="/home/dashboard" class="btn btn-block btn-primary text-uppercase">Chọn</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
</body>

</html>